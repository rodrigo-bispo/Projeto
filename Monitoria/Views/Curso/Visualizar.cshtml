
@{
    ViewBag.Title = "Visualizar";
}

<h2>Tarefa</h2>

<div class="#">
    <div class="panel panel-primary" id="painel-enquete">
        <div class="panel-heading">
            <p>Questões</p>
        </div>
        <div class="panel-body">
            <p>O que é um Mouse :</p>
            <input type="radio" name="opcao" value="1" checked /> Um Rato<br />
            <input type="radio" name="opcao" value="2" /> Não sei<br />
            <input type="radio" name="opcao" value="3" /> Pula<br />

            <br>

            <p>O que é um Teclado :</p>
            <input type="radio" name="opcao" value="1" checked /> Bispo<br />
            <input type="radio" name="opcao" value="2" /> rodrigo<br />
            <input type="radio" name="opcao" value="3" /> Hoje<br />

            <br>

            <p>O que é uma TV :</p>
            <input type="radio" name="opcao" value="1" checked /> Netflix<br />
            <input type="radio" name="opcao" value="2" /> Youtube<br />
            <input type="radio" name="opcao" value="3" /> watch<br />
        </div>
        <div class="panel-footer">
            <button class="btn btn-primary" id="btnEnviarEnquete">Enviar</button>
        </div>
    </div>
</div>

<br>
@Html.ActionLink("Volta ao curso", "Index", "Curso")



