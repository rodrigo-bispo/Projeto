@model IEnumerable<Monitoria.Models.Semestre>

@{
    ViewBag.Title = "Index";
}

<h2>Semestre</h2>

<p>
    @Html.ActionLink("Novo Semestre", "Create")
</p>
<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeSemestre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomeSemestre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.SemestreID }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.SemestreID }) |
            @Html.ActionLink("Apagar", "Delete", new { id=item.SemestreID })
        </td>
    </tr>
}

</table>
